import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
from sklearn.linear_model import LinearRegression
from sklearn.model_selection import train_test_split
from sklearn.metrics import mean_squared_error , r2_score



#predict grade changes of student according to their absent days



data = pd.read_csv('student.csv')


data.head()


data.columns


#Define Feature and Target Variable
Xvar = 'absences'
Yvar = 'G3'


X = data[[Xvar]]
Y = data[Yvar]


#Train and test Data
X_train ,X_test,Y_train,Y_test = train_test_split(
    X,Y,test_size = 0.2 ,random_state = 42
)


#Fit data in model
model = LinearRegression()


model.fit(X_train,Y_train)


Y_pred = model.predict(X_test)


Y_pred


beta0 = model.intercept_


beta1 = model.coef_[0]


print(model.intercept_.shape)
print(model.coef_.shape)


print(f"Intercept:{beta0}")
print(f"Slope :{beta1}")


#Calculate Metrices


rmse = np.sqrt(mean_squared_error(Y_test,Y_pred))


r2 = r2_score(Y_test,Y_pred)


print(rmse)


print(r2)


sns.scatterplot(x = X_test[Xvar],y=Y_test,color='blue')
plt.plot(X_test[Xvar],Y_pred,color='red')


#Predict
new_absent_data = {
    'absences': [10,20,30]
}


X_new = pd.DataFrame(new_absent_data)


X_new


Y_new_pred = model.predict(X_new)


Y_new_pred


X_new['Predicted_G3'] = Y_new_pred


X_new






'''
Aggregate Function 
-------------------
Mean ->Calculates average record
Max->Extracts maximum value from column
Sum->Calculates total value for column
Min->Extracts minimum value from column
Count->Count total number of record
'''


import pandas as pd



df = pd.read_csv("Datas/Employees.csv")


df.head()


df.info()


df.shape


#KPI(Key Performance Indicatar)


#Find total average salary of employees
average_salary = df['SALARY'].mean()
print(f"{average_salary:.2f}")


#Find total salary
total_salary = df['SALARY'].sum()
total_salary


#Find maximum salary
Highest = df['SALARY'].max()
Highest


#Find minimum salary
Lowest = df['SALARY'].min()
Lowest


#Count 
count_salary = df['SALARY'].count()
count_salary


total_unique_salary = df['SALARY'].nunique()
total_unique_salary


job_count = df['JOB_ID'].nunique()
job_count


job_count = df['JOB_ID'].value_counts()
job_count






